{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "build",
      "program": "${workspaceRoot}/scripts/build.js",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "start",
      "program": "${workspaceRoot}/scripts/start.js",
      "cwd": "${workspaceRoot}",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test",
      "program": "${workspaceRoot}/scripts/test.js",
      "cwd": "${workspaceRoot}",
      "env": {
        "DEBUG": "webjerk*"
      },
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
